#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/Users/bytedance/repos/ts-lib-library/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="$NODE_PATH:/Users/bytedance/repos/ts-lib-library/node_modules/.pnpm/node_modules"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../../../../registry.npmjs.org+typescript@4.7.4/node_modules/typescript/bin/tsc" "$@"
else
  exec node  "$basedir/../../../../../registry.npmjs.org+typescript@4.7.4/node_modules/typescript/bin/tsc" "$@"
fi
