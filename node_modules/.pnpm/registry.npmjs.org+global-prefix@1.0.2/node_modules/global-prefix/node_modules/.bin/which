#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/Users/bytedance/repos/ts-lib-library/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="$NODE_PATH:/Users/bytedance/repos/ts-lib-library/node_modules/.pnpm/node_modules"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../../../../registry.npmjs.org+which@1.3.1/node_modules/which/bin/which" "$@"
else
  exec node  "$basedir/../../../../../registry.npmjs.org+which@1.3.1/node_modules/which/bin/which" "$@"
fi
